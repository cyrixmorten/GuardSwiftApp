<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:animateLayoutChanges="true"
    android:descendantFocusability="blocksDescendants">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tv_queMsg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:padding="5dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:background="@color/red_btn_bg_color"
            android:text="@string/alarm_que_msg"
            android:textStyle="bold"/>

        <RelativeLayout
            android:id="@+id/body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_below="@+id/tv_queMsg"
            android:padding="5dp"
            android:paddingLeft="15dp" >

            <TextView
                android:id="@+id/securityLevel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:paddingBottom="5dp"
                android:text="10"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/alarmDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="5dp"
                android:layout_alignParentRight="true"
                android:text="24-4-2015"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <!--<com.beardedhen.androidbootstrap.FontAwesomeText
                android:id="@+id/timeIcon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_below="@+id/alarmDate"
                android:layout_alignParentRight="true"
                android:textColor="@color/Grey"
                android:textSize="18sp"
                fontawesometext:fa_icon="fa-clock-o" />-->

            <TextView
                android:id="@+id/alarmTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="5dp"
                android:text="15:23"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/alarmDate"
                />

            <TextView
                android:id="@+id/clientName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/alarmDate"
                android:text="Lego"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/zone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:paddingTop="2dp"
                android:paddingBottom="2dp"
                android:layout_below="@+id/clientName"
                android:ellipsize="end"
                android:text="Zone"
                android:maxLines="3"
                android:textAppearance="?android:attr/textAppearanceMedium"/>

            <TextView
                android:id="@+id/clientAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/zone"
                android:text="Adresse"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
                android:id="@+id/clientAddress2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/clientAddress"
                android:text="Adresse"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
                android:id="@+id/alarmType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/clientAddress2"
                android:paddingTop="5dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/linearLayout_distance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignRight="@+id/task_planned_timespan"
                android:layout_alignTop="@+id/alarmType"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/distanceValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:paddingRight="5dp"
                    android:text="5 km"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/distanceType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall" />
            </LinearLayout>
        </RelativeLayout>

